{
    "name": "Color picker",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons":{
        "16":"images/16_icon.png",
        "32":"images/32_icon.png",
        "64":"images/64_png.png"
    }
    
}